#!/bin/sh

# places i often keep bin files:
export PATH="/usr/local/bin:.:$HOME/software/bin:$HOME/bin:$PATH:./bin"

# choice editor, pager
export PAGER="less -R"
export EDITOR="vim"
export GIT_PAGER="less -R"

# python stuff
export PYTHONSTARTUP="$HOME/.pythonrc.py"
export PYTHONIOENCODING="utf-8"


