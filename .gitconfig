[color]
	branch = auto
	diff = auto
	status = auto
[color "branch"]
	current = yellow reverse
	local = yellow
	remote = green
[color "diff"]
	meta = blue bold
	frag = magenta bold
	old = red bold
	new = green bold
[color "status"]
	added = yellow
	changed = green
	untracked = cyan
[color]
	ui = true
[core]
	WHITESPACE=fix,-indent-with-non-tab,trailing-space,cr-at-eol
	pager = cat
[github]
	user = stephenroller
	token = dd7abe231add6a341eccbd5c791a6bcc
[user]
	name = Stephen Roller
	email = roller@fb.com
[alias]
	co = checkout
	branches = branch --list
	st = status
	histedit = rebase -i origin/master
	amend = commit --amend
[push]
	default = current
[pull]
	rebase = true
